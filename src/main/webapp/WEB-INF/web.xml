<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Netmanager Web Application</display-name>
	<servlet>
		<servlet-name>NetmanagerServlet</servlet-name>
		<display-name>NetmanagerServlet</display-name>
		<description></description>
		<servlet-class>com.disclinc.netmanager.servlet.NetmanagerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MenuServlet</servlet-name>
		<display-name>MenuServlet</display-name>
		<description></description>
		<servlet-class>com.disclinc.netmanager.servlet.MenuServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PacienteServlet</servlet-name>
		<display-name>PacienteServlet</display-name>
		<description></description>
		<servlet-class>com.disclinc.netmanager.servlet.PacienteServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>NetmanagerServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MenuServlet</servlet-name>
		<url-pattern>/menu</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PacienteServlet</servlet-name>
		<url-pattern>/paciente</url-pattern>
	</servlet-mapping>
</web-app>
